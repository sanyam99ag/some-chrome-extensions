 
{
    "name": "Word Count",
    "version": "1.0",
    "manifest_version": 2,
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "browser_action": {
      "default_title": "Word_Count"
    },
    "background": {
      "scripts": ["background.js"]
    },
    "permissions": ["tabs"]
  }